{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-config-airbnb",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "no-unused-expression": true,
    "no-unused-variable": [
      true,
      { "ignore-pattern": "React" }
    ],
    "import-name": [false],
    "function-name": false,
    "variable-name": false,
    "jsx-boolean-value": ["never"],
    "jsx-no-lambda": false
  },
  "rulesDirectory": []
}